    "test-chroma": "node --import tsx scripts/test-chroma.js",
    "ingest-files": "set NODE_OPTIONS=--max-old-space-size=4096 && tsx src/scripts/ingestFiles.ts",
    "ingest": "node --loader ts-node/esm src/scripts/simpleIngest.ts"
